<+>{% extends 'layout/non_base.html' %}\r\n\r\n{% block title %}Sign in & Sign up{% endblock %}\r\n\r\n{% block css %}\r\n    <link rel=\"stylesheet\" href=\"{{ url_for('static', filename='css/auth.css') }}\">\r\n{% endblock %}\r\n\r\n{% block js_body %}\r\n    <script>\r\n        const signInBtn = document.querySelector(\"#sign-in-btn\");\r\n        const signUpBtn = document.querySelector(\"#sign-up-btn\");\r\n        const container = document.querySelector(\".my-container\");\r\n        const btnForgotPass = document.querySelector(\".forgot-password-btn\");\r\n        const modalForgotPass = document.querySelector(\".modal-forgot-password\");\r\n        const modalForgotPassWrapper = document.querySelector(\".modal-forgot-password .wrapper\");\r\n        signUpBtn.addEventListener(\"click\", () => {\r\n            container.classList.add(\"sign-up-mode\");\r\n        });\r\n\r\n        signInBtn.addEventListener(\"click\", () => {\r\n            container.classList.remove(\"sign-up-mode\");\r\n        });\r\n\r\n        btnForgotPass.addEventListener(\"click\", function () {\r\n            modalForgotPass.style.display = \"block\";\r\n        })\r\n\r\n        modalForgotPass.addEventListener(\"click\", function () {\r\n            modalForgotPass.style.display = \"none\";\r\n        })\r\n\r\n        modalForgotPassWrapper.addEventListener(\"click\", function (e) {\r\n            e.stopPropagation();\r\n        })\r\n    </script>\r\n{% endblock %}\r\n\r\n{% block content %}\r\n    <div class=\"my-container\">\r\n        <div class=\"forms-container\">\r\n            <div class=\"signin-signup\">\r\n                <form method=\"post\" action=\"\" class=\"sign-in-form\">\r\n                    <h2 class=\"title\">Sign in</h2>\r\n                    <div class=\"input-field\">\r\n                        <i class=\"fas fa-user\"></i>\r\n                        <input type=\"text\" name=\"username_signin\" placeholder=\"Username\">\r\n                    </div>\r\n\r\n                    <div class=\"input-field\">\r\n                        <i class=\"fas fa-lock\"></i>\r\n                        <input type=\"password\" name=\"password_signin\" placeholder=\"Password\">\r\n                    </div>\r\n                    <button class=\"btn-signin-and-signup\">Sign in</button>\r\n                    <a href=\"#\" class=\"forgot-password-btn\">Forgot password</a>\r\n                    <p class=\"social-text\">Or sign in with social platforms</p>\r\n                    <div class=\"social-media\">\r\n                        <button class=\"social-icon\"><i class=\"fab fa-facebook-f\"></i></button>\r\n                        <button class=\"social-icon\"><i class=\"fab fa-google\"></i></button>\r\n                    </div>\r\n                </form>\r\n\r\n                <form method=\"post\" action=\"\" class=\"sign-up-form\">\r\n                    <h2 class=\"title\">Sign up</h2>\r\n\r\n                    <div class=\"input-field-double\">\r\n                        <div class=\"row\">\r\n                            <div class=\"my-col-6\">\r\n                                <div class=\"input-fl-name\">\r\n                                    <i class=\"fa-solid fa-signature\"></i>\r\n                                    <input type=\"text\" name=\"firstname_signup\" placeholder=\"First name\">\r\n                                </div>\r\n                            </div>\r\n\r\n                            <div class=\"my-col-6\">\r\n                                <div class=\"input-fl-name\">\r\n                                    <i class=\"fa-solid fa-signature\"></i>\r\n                                    <input type=\"text\" name=\"lastname_signup\" placeholder=\"Last name\">\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div class=\"input-field\">\r\n                        <i class=\"fas fa-user\"></i>\r\n                        <input type=\"text\" name=\"username_signup\" placeholder=\"Username\">\r\n                    </div>\r\n\r\n                    <div class=\"input-field\">\r\n                        <i class=\"fas fa-lock\"></i>\r\n                        <input type=\"password\" name=\"password_signup\" placeholder=\"Password\">\r\n                    </div>\r\n\r\n                    <div class=\"input-field\">\r\n                        <i class=\"fas fa-lock\"></i>\r\n                        <input type=\"password\" name=\"confirm_password_signup\" placeholder=\"Confirm password\">\r\n                    </div>\r\n\r\n                    <div class=\"input-field\">\r\n                        <i class=\"fa-solid fa-envelope\"></i>\r\n                        <input type=\"email\" name=\"email_signup\" placeholder=\"Email\">\r\n                    </div>\r\n\r\n                    <div class=\"input-field\">\r\n                        <i class=\"fa-solid fa-phone\"></i>\r\n                        <input type=\"tel\" name=\"phone_signup\" placeholder=\"Phone\">\r\n                    </div>\r\n\r\n                    <button class=\"btn-signin-and-signup\">Sign up</button>\r\n                    <p class=\"social-text\">Or sign up with social platforms</p>\r\n                    <div class=\"social-media\">\r\n                        <button class=\"social-icon\"><i class=\"fab fa-facebook-f\"></i></button>\r\n                        <button class=\"social-icon\"><i class=\"fab fa-google\"></i></button>\r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"panels-container\">\r\n            <div class=\"panel left-panel\">\r\n                <div class=\"content\">\r\n                    <h3>Do not have an account ?</h3>\r\n                    <p>Please sign up to become a member of F&H.</p>\r\n                    <button class=\"my-btn btn-animation animation-transparent\" id=\"sign-up-btn\">Sign up</button>\r\n                </div>\r\n                <img src=\"{{ url_for('static', filename='images/signup.svg') }}\" class=\"image\" alt=\"Sign up\"/>\r\n            </div>\r\n            <div class=\"panel right-panel\">\r\n                <div class=\"content\">\r\n                    <h3 class=\"none\">Do you already have an account ?</h3>\r\n                    <p class=\"none\">Please sign in to receive many F&H offers.</p>\r\n                    <button class=\"my-btn btn-animation animation-transparent\" id=\"sign-in-btn\">Sign in</button>\r\n                </div>\r\n                <img src=\"{{ url_for('static', filename='images/signin.svg') }}\" class=\"image\" alt=\"\"/>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <div class=\"modal-forgot-password\">\r\n        <div class=\"wrapper\">\r\n            <form method=\"post\" action=\"\" class=\"forgot-password-form\">\r\n                <h2 class=\"forgot-password-form--title\">Forgot password</h2>\r\n                <p class=\"forgot-password-form--des\">Enter your email address</p>\r\n                <input type=\"text\" name=\"forgotpssw_info\" placeholder=\"Email\" class=\"input-style\">\r\n                <button class=\"button\"><span class=\"button-content\">Send</span></button>\r\n            </form>\r\n        </div>\r\n    </div>\r\n{% endblock %}
